####### Compile

chatServer.o: chatServer.cpp ServerWindow.h \
		ThAccept.h \
		ChatException.h \
		ServerSocket.h \
		ThClient.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o chatServer.o chatServer.cpp

ServerWindow.o: ServerWindow.cpp ServerWindow.h \
		ThAccept.h \
		ChatException.h \
		ServerSocket.h \
		ThClient.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ServerWindow.o ServerWindow.cpp

ChatException.o: ChatException.cpp ChatException.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ChatException.o ChatException.cpp

ServerSocket.o: ServerSocket.cpp ServerSocket.h \
		ChatException.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ServerSocket.o ServerSocket.cpp

ThAccept.o: ThAccept.cpp ThAccept.h \
		ChatException.h \
		ServerSocket.h \
		ThClient.h \
		ServerWindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ThAccept.o ThAccept.cpp

ThClient.o: ThClient.cpp ThClient.h \
		ServerWindow.h \
		ThAccept.h \
		ChatException.h \
		ServerSocket.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ThClient.o ThClient.cpp

moc_ServerWindow.o: moc_ServerWindow.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_ServerWindow.o moc_ServerWindow.cpp

moc_ThAccept.o: moc_ThAccept.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_ThAccept.o moc_ThAccept.cpp

